{
  "appId": "com.linedup.app",
  "appName": "LinedUp",
  "webDir": "dist",
  "server": {
    "url": "https://linedup-app.vercel.app",
    "androidScheme": "https",
    "iosScheme": "https"
  },
  "ios": {
    "contentInset": "never",
    "preferredContentMode": "mobile",
    "scheme": "LinedUp",
    "backgroundColor": "#0C0F1D"
  },
  "plugins": {
    "StatusBar": {
      "style": "LIGHT",
      "backgroundColor": "#0C0F1D",
      "overlaysWebView": true
    }
  }
}
